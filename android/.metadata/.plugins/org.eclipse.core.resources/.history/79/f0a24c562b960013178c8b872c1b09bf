package com.trashcam.model;

import java.io.File;
import java.util.ArrayList;

public class DeleteFileModelManager {
	private DeleteFileModelManager instance;

	public ArrayList<DeleteFileModel> toBeDeleted;

	private DeleteFileModelManager() {
		toBeDeleted = new ArrayList<DeleteFileModel>();
		instance = this;
	}
	
	public DeleteFileModelManager getInstance() {
		return instance;
	}
	public void addFile(File new_picture, "3"){
		toBeDeleted.add(new DeleteFileModel(new_picture, dateToBeDeleted))
	}
}
